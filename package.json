{
  "name": "servuslp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.0",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.0",
    "pluralize": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.2.3",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.3.5",
    "react-google-maps": "^9.0.1",
    "react-image-gallery": "^0.8.3",
    "react-loadable": "^4.0.5",
    "react-owl-carousel": "^2.2.0",
    "react-router-bootstrap": "^0.24.2",
    "react-router-breadcrumbs": "^2.1.1",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.11",
    "react-slick": "^0.15.4",
    "react-static": "^4.3.4",
    "react-transition-group": "^2.2.1",
    "react-youtube": "^7.4.0",
    "recompose": "^0.26.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "predeploy": "yarn build",
    "deploy": "aws s3 sync build/ s3://$S3_BUCKET",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_ID --paths '/*' && aws cloudfront create-invalidation --distribution-id $WWW_CLOUDFRONT_ID --paths '/*'",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "react-router-sitemap": "^1.1.2"
  },
  "eslintConfig": {
    "plugins": [
      "react"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ]
  }
}
